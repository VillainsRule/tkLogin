{
    "name": "tklogin",
    "type": "module",
    "scripts": {
        "dev": "bun cli/localSSR.ts",
        "build": "bun buildclient && bun buildserver",
        "deploy": "bunx wrangler pages deploy --commit-dirty=true",
        "prod": "bun run build && bun cli/prepareProd.js && bun run deploy",
        "buildclient": "bunx --bun vite build --outDir dist/client",
        "buildserver": "bunx --bun vite build --ssr src/entry-server.jsx --outDir dist/server",
        "test": "bunx wrangler pages dev"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.4",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "fuzzysort": "^3.1.0",
        "lucide-react": "^0.562.0",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "tailwindcss-animate": "^1.0.7",
        "tw-animate-css": "^1.4.0"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.18",
        "@types/bun": "^1.3.5",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "turnstile-types": "^1.2.3",
        "typescript": "^5.9.3",
        "vite": "8.0.0-beta.1",
        "wrangler": "^4.54.0"
    }
}